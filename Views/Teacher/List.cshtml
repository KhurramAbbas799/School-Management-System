@model List<SchoolManagementSystem.Models.Teacher>
@{
	ViewData["Title"] = "List";
}

<table>

	<tr>

		<th>Id</th>
		<th>Name</th>
		<th>Email</th>
		<th>Contact</th>
		<th>Gender</th>
		<th>Actions</th>
	</tr>


	@for (int i = 0; i < Model.Count; i++)
	{

		<tr>

			<td>@Model[i].TeacherId </td>
			<td>@Model[i].TeacherName </td>
			<td>@Model[i].TeacherEmail </td>
			<td>@Model[i].TeacherContact </td>
			<td>@Model[i].TeacherGender </td>
			<td> <a href="@Url.Action("Delete", new{Id=@Model[i].TeacherId})">  Delete</a>  </td>

			<td>
				<a href="@Url.Action("Index", new{@Model[i].TeacherId,
		@Model[i].TeacherName, @Model[i].TeacherEmail, @Model[i].TeacherContact,
               @Model[i].TeacherGender  })"> Edit</a>
			</td>
		</tr>
	}

</table>
