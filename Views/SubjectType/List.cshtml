@model List<SchoolManagementSystem.Models.SubjectType>
@{
	ViewData["Title"] = "List";
}

<table>

	<tr>

		<th>Id</th>
		<th>Name</th>

	</tr>


	@for (int i = 0; i < Model.Count; i++)
	{

		<tr>

			<td>@Model[i].SubjectTypeId</td>
			<td>@Model[i].SubjectTypeName</td>
			<td> <a href="@Url.Action("Delete", new{Id=@Model[i].SubjectTypeId})">  Delete</a>  </td>
			<td>
				<a href="@Url.Action("Index", new{@Model[i].SubjectTypeId,
		@Model[i].SubjectTypeName})"> Edit</a>
			</td>
		</tr>
	}

</table>
