@model SchoolManagementSystem.DTO.Dto

@{
	ViewData["Title"] = "Rider";
}

<form method="post" asp-action="AddRider">

	<label>FirstName</label>
	<input type="hidden" asp-for="RiderId"/>
		<input type="text" asp-for="FirstName" />
	<br />

	<label>LastName</label>
<input type="text" asp-for="LastName" />
	<br />

	
	
	<label>Password</label>
	<input type="text" asp-for="Password" />
	<br />



	<label>Email</label>
<input type="text" asp-for="Email" />
	<br />

	
	<label>Contact</label>
<input type="text" asp-for="Contact" />
	<br />

	
	<label>CNIC</label>
	<input type="text" asp-for="CNIC" />
	<br />

	<label>Address</label>
<input type="text" asp-for="Address" />
	<br />


	<label>Transport Type</label>
	<input type="text" asp-for="TransportType" />
	<br />

	<button type="submit">Submit</button>
</form>